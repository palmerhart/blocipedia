<h1>All Wiki's</h1>

<button><%= link_to "New Wiki", new_wiki_path %></button>

<% @wiki.each do |wiki| %>
    <div class="media">
        <div class="media-body">
            <h4 class="media-heading">
                <p><%= link_to wiki.title, wiki %></p>
            </h4>
        </div>
    </div>
<% end %>
